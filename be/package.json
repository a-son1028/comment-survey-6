  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build-babel": "babel src -s -d dist",
    "build": "npm run clean && npm run build-babel",
    "start": "node dist/app.js",
    "dev": "babel-watch src/app.js",
    "create-files": "babel --max_old_space_size=8192 createTxtFile.js",
    "export": "babel-watch --max_old_space_size=8192 export.js",
    "export-distance": "babel-watch --max_old_space_size=8192 exportDistance.js",
    "export-distanceLeafNodes": "babel-watch --max_old_space_size=8192 exportDistanceLeafNodesAndFirstLevel.js",
    "statistics": "babel-watch --max_old_space_size=8192 statistics.js",
    "statistics_stage1": "babel-watch --max_old_space_size=8192 statistics_stage1.js",
    "statistics_stage2": "babel-watch --max_old_space_size=8192 statistics_stage2.js",
    "statistics_stage3": "babel-watch --max_old_space_size=8192 statistics_stage3.js",
    "statistics_stage_expert": "babel-watch --max_old_space_size=8192 statistics_stage_expert.js",
    "thirdFile": "babel-watch --max_old_space_size=8192 thirdFile.js",
    "countingRisk": "babel-watch --max_old_space_size=8192 countingRisk.js",
    "seed": "babel-node --max_old_space_size=8192 src/seed/comment.js",
    "stat": "babel-node --max_old_space_size=20192 src/modules/stat.js",
    "bibtext": "babel-node --max_old_space_size=20192 src/modules/bibtext.js",
    "getdataitem": "babel-node --max_old_space_size=12192 modules/getdataitem.js",
    "getcomment": "babel-node --max_old_space_size=12192 src/modules/getcomment.js",
    "getcommentProd": "node --expose-gc --max_old_space_size=12192 dist/modules/getcomment.js",
    "test": "babel-node --max_old_space_size=12192 src/modules/test.js",
    "test2": "babel-watch src/modules/test2.js",
    "test3": "babel-watch src/modules/test3.js",
    "test2-node": "node --max_old_space_size=12192 dist/modules/test2.js"
  },
  "heroku-prebuild": "rm -rf node_modules",
